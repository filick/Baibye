{
    "manifest_version": 2,

    "name": "Baibye",
    "description": "Say goodbye to Baidu's awful ads.",
    "version": "1.2",

    "icons": {
        "128": "icon/logo128.png"
    },

    "content_scripts": [{
        "matches": ["https://www.baidu.com/*", "http://www.baidu.com/*"],
        "js": ["js/jquery-1.12.3.min.js", "js/ad.js"],
        "run_at": "document_end"
    }]

}